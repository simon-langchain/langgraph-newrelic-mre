{
  "dependencies": ["./newrelic.ini"],
  "graphs": {
    "agent": "./agent.py:graph"
  },
  "env": ".env",
  "python_version": "3.11",
  "dockerfile_lines": [
    "ENV NEW_RELIC_CONFIG_FILE=/deps/newrelic.ini"
  ],
  "http": {
    "disable_studio_auth": true,
    "allow_origins": "*",
    "allow_methods": "*",
    "allow_headers": "*",
    "configurable_headers": {
      "exclude": ["*"],
      "include": ["authorization"]
    }
  }
}

